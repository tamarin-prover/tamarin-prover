rule (modulo E) Terminal_Receives_Records_NoLocalAuth_C8:
   [
   Terminal_Sent_ReadRecord_C8( $Terminal, <'NoLocalAuth', AIP_Data2>,
                                blindedQC, dT, r, SKc, SKi, UN, DRRE
   ),
   In( <senc(<~PAN, expDate, <'14', SDA_Hash, QC>, sign4>, SKc), $CA, 
        <'02', $Issuer, pubkIssuer, $CA>, sign2>
   ),
   !IssuingCA( $Issuer, $CA ),
   !CertCA( $CA, <<'01', $CA, pubkCA, $CA>, sign1> )
   ]
  -->
   [
   Terminal_Ready_For_Cryptogram_C8( $Terminal, ~PAN, $Issuer, $CA,
                                     <'NoLocalAuth', AIP_Data2>, pubkIssuer, UN, SKc, SKi, expDate, DRRE
   )
   ]

  /* has exactly the trivial AC variant */

rule (modulo E) Terminal_Sends_GenerateAC_C8:
   [
   Terminal_Ready_For_Cryptogram_C8( $Terminal, ~PAN, $Issuer, $CA, AIP,
                                     pubkIssuer, UN, SKc, SKi, expDate, DRRE
   ),
   !Value( $amount, value ), In( <acType, CVM> )
   ]
  --[ ValidCVM( CVM ) ]->
   [
   Out( <'GENERATE_AC', acType, $amount, 'Switzerland', 'TVR', 'CHF', 
