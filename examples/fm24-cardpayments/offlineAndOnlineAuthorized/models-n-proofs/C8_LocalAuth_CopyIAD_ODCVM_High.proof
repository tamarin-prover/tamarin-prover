lemma issuer_accepts:
  all-traces
  " t #i.
    (TerminalAccepts( t ) @ #i) 
    ((( #j. IssuerDeclines( t ) @ #j)) 
     ( A #k. (Honest( A ) @ #i)  (Compromise( A ) @ #k)))"
/*
guarded formula characterizing all counter-examples:
" t #i.
  (TerminalAccepts( t ) @ #i)
 
  ( #j. (IssuerDeclines( t ) @ #j)) 
  ( A #k. (Honest( A ) @ #i)  (Compromise( A ) @ #k)  )"
*/
simplify
solve( TerminalAccepts( t ) @ #i )
  case Terminal_Commits_ARQC_C8
  solve( Recv( $Issuer, $Terminal, <~channelID, '2'>, <'ARC', ARPC>
         )  #i )
    case Terminal_Issuer_Network_case_1
    solve( Recv( $Terminal, $Issuer, <~channelID, '1'>,
                 <
                  <~PAN.1, <'LocalAuth', 'CopyIAD'>, CVM, 
                   <$amount.1, country, TVR, currency, date, type, UN.1, CVMR.1, TRMD.1>, 
                   ATC.1, 
