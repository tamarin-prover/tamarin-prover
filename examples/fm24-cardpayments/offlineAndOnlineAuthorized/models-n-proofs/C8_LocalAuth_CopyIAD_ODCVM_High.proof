restriction equal:
  " a b #i. (Eq( a, b ) @ #i)  (a = b)"
  // safety formula

restriction not_equal:
  " a #i. (NEq( a, a ) @ #i)  ()"
  // safety formula

restriction once:
  " a #i #j. ((Once( a ) @ #i)  (Once( a ) @ #j))  (#i = #j)"
  // safety formula

restriction unique_role:
  " A r1 r2 #i #j.
    ((Role( A, r1 ) @ #i)  (Role( A, r2 ) @ #j))  (r1 = r2)"
  // safety formula

restriction valid_CVM:
  " CVM #i.
    (ValidCVM( CVM ) @ #i) 
    (((CVM = 'NoCVM')  (CVM = 'OnlinePIN'))  (CVM = 'ODCVM'))"
  // safety formula

