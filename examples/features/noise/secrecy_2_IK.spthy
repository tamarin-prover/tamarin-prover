/* Handshake initialisation with initial
        knowledge for Initiator */

rule Init_Handshake_Initiator:
 let hash0 = h(<'IK', prologue>)
     hash1 = h(<hash0, ('g'^rs)>) in
 [ Init_Handshake_Initiator(id, prologue, ~s, ('g'^rs)) ]
 --[  ]->
 [ HandshakeState_Initiator_0(id, hash1, 'IK', ~s, ('g'^rs)) ]

/* Initiator is Snding this message: -> e es s ss */

rule Handshake_Initiator_Snd_1:
 let hash1 = h(<hash, ('g'^~e0)>)
     ck2 = kdf1(<ck, (('g'^rs)^~e0)>)
     k3 = kdf2(<ck, (('g'^rs)^~e0)>)
     c4 = aead(k3, '0', hash1, ('g'^~s))
     hash5 = h(<hash1, c4>)
     ck6 = kdf1(<ck2, (('g'^rs)^~s)>)
     k7 = kdf2(<ck2, (('g'^rs)^~s)>)
     c8 = aead(k7, '0', hash5, payload)
     hash9 = h(<hash5, c8>) in
 [ HandshakeState_Initiator_0(id, hash, ck, ~s, ('g'^rs)), Handshake_Snd_1(id, payload), Ek(~e0) ]
 --[ RawSnd('Initiator1', <('g'^~e0), c4, c8>) ]->
 [
  MyEk_Initiator(id, ('g'^~e0)),
  HandshakeState_Initiator_1(id, hash9, ck6, k7, ~e0, ~s, ('g'^rs)),
  Send(<('g'^~e0), c4, c8>)
 ]

/* Initiator is Recving this message: <- e ee se */

rule Handshake_Initiator_Recv_2:
 let hash1 = h(<hash, ('g'^re0)>)
     ck2 = kdf1(<ck, (('g'^re0)^~e)>)
     k3 = kdf2(<ck, (('g'^re0)^~e)>)
     ck4 = kdf1(<ck2, (('g'^re0)^~s)>)
     k5 = kdf2(<ck2, (('g'^re0)^~s)>)
     c6 = aead(k5, '0', hash1, payload)
     hash7 = h(<hash1, c6>) in
 [ HandshakeState_Initiator_1(id, hash, ck, k, ~e, ~s, ('g'^rs)), Recv(<('g'^re0), c6>) ]
 --[
  Secret_IK_2_Initiator(('g'^~e), ('g'^~s), ('g'^re0), ('g'^rs), payload),
  RawRecv('Responder2', <('g'^re0), c6>)
 ]->
 [
  PeerEk_Initiator(id, ('g'^re0)),
  HandshakeState_Initiator_2(id, hash7, ck4, k5, ~e, ('g'^re0), ~s, ('g'^rs)),
  Handshake_Recv_2(id, payload)
 ]

/* Initiator switches to transport mode */

rule Initiator_to_transport_mode:
 [ HandshakeState_Initiator_2(id, hash, ck, k, ~e, ('g'^re), ~s, ('g'^rs)) ]
 --[  ]->
 [ !Transport_Initiator(id, kdf1(ck), kdf2(ck)), !SessionId(id, hash) ]

/* Handshake initialisation with initial
        knowledge for Responder */

rule Init_Handshake_Responder:
 let hash0 = h(<'IK', prologue>)
     hash1 = h(<hash0, ('g'^~s)>) in
 [ Init_Handshake_Responder(id, prologue, ~s) ]
 --[  ]->
 [ HandshakeState_Responder_0(id, hash1, 'IK', ~s) ]

/* Responder is Recving this message: -> e es s ss */

rule Handshake_Responder_Recv_1:
 let hash1 = h(<hash, ('g'^re0)>)
     ck2 = kdf1(<ck, (('g'^re0)^~s)>)
     k3 = kdf2(<ck, (('g'^re0)^~s)>)
     c5 = aead(k3, '0', hash1, ('g'^rs4))
     hash6 = h(<hash1, c5>)
     ck7 = kdf1(<ck2, (('g'^rs4)^~s)>)
     k8 = kdf2(<ck2, (('g'^rs4)^~s)>)
     c9 = aead(k8, '0', hash6, payload)
     hash10 = h(<hash6, c9>) in
 [ HandshakeState_Responder_0(id, hash, ck, ~s), Recv(<('g'^re0), c5, c9>) ]
 --[ RawRecv('Initiator1', <('g'^re0), c5, c9>) ]->
 [
  PeerEk_Responder(id, ('g'^re0)),
  PeerLtk_Responder(id, ('g'^rs4)),
  HandshakeState_Responder_1(id, hash10, ck7, k8, ('g'^re0), ~s, ('g'^rs4)),
  Handshake_Recv_1(id, payload)
 ]

/* Responder is Snding this message: <- e ee se */

rule Handshake_Responder_Snd_2:
 let hash1 = h(<hash, ('g'^~e0)>)
     ck2 = kdf1(<ck, (('g'^re)^~e0)>)
     k3 = kdf2(<ck, (('g'^re)^~e0)>)
     ck4 = kdf1(<ck2, (('g'^rs)^~e0)>)
     k5 = kdf2(<ck2, (('g'^rs)^~e0)>)
     c6 = aead(k5, '0', hash1, payload)
     hash7 = h(<hash1, c6>) in
 [
  HandshakeState_Responder_1(id, hash, ck, k, ('g'^re), ~s, ('g'^rs)),
  Handshake_Snd_2(id, payload),
  Ek(~e0)
 ]
 --[
  Secret_IK_2_Responder(('g'^~e0), ('g'^~s), ('g'^re), ('g'^rs), payload),
  RawSnd('Responder2', <('g'^~e0), c6>)
 ]->
 [
  MyEk_Responder(id, ('g'^~e0)),
  HandshakeState_Responder_2(id, hash7, ck4, k5, ~e0, ('g'^re), ~s, ('g'^rs)),
  Send(<('g'^~e0), c6>)
 ]

/* Responder switches to transport mode */

rule Responder_to_transport_mode:
 [ HandshakeState_Responder_2(id, hash, ck, k, ~e, ('g'^re), ~s, ('g'^rs)) ]
 --[  ]->
 [ !Transport_Responder(id, kdf1(ck), kdf2(ck)), !SessionId(id, hash) ]

/* transport mode rules */

rule I_Recv_from_R:
 let c = <n, aead(kr2i, n, '0', msg)>
 in
 [ Recv(c)
 , !Transport_Initiator(id, ki2r, kr2i)
 ] --[
 RawRecv(<'R', n>, c)
 ]->
 [ I_Recv(id, n, msg) ]

rule R_Recv_from_I:
 let c = <n, aead(ki2r, n, '0', msg)>
 in
 [ Recv(c)
 , !Transport_Responder(id, ki2r, kr2i)
 ] --[
 RawRecv(<'I', n>, c)
 ]->
 [ R_Recv(id, n, msg) ]

rule I_Snd_to_R:
 let c = <n, aead(ki2r, n, '0', msg)>
 in
 [ I_Snd(id, n, msg)
 , !Transport_Initiator(id, ki2r, kr2i)
 ] --[
 RawSnd(<'I', n>, c)
 ]->
 [ Send(c)
 ]

rule R_Snd_to_I:
 let c = <n, aead(kr2i, n, '0', msg)>
 in
 [ R_Snd(id, n, msg)
 , !Transport_Responder(id, ki2r, kr2i)
 ] --[
 RawSnd(<'R', n>, c)
 ]->
 [ Send(c)
 ]


/* Start of instrumentation protocol */

rule channel_send:
[ Send(m) ] --> [ Out(m) ]
rule channel_receive:
[ In(m) ] --> [ Recv(m) ]


rule disclose_sid: [ !SessionId(~id, h) ] --> [ Out(h) ]

rule static_keygen:
 [ Fr(~key) ] --[ HonestPk(('g'^~key)) ]-> [ !Pk(~key, ('g'^~key)), Out(('g'^~key)) ]

rule static_reveal:
 [ !Pk(~s, ('g'^~s)) ] --[ RevealLtk(('g'^~s)) ]-> [ Out(~s) ]

rule ephemeral_keygen:
 [ Fr(~e) ] --[ HonestEk(('g'^~e)) ]-> [ Ek(~e), Out(('g'^~e)), RevealableEk(~e) ]

rule ephemeral_reveal:
 [ RevealableEk(~e) ] --[ RevealEk(('g'^~e)) ]-> [ Out(~e) ]

/* Creation of the Initiator */

rule Create_Initiator:
 [ Fr(~id), !Pk(~ltk, me), In(prologue), In(peer) ]
 --[ Create_Initiator(), Peer(peer) ]->
 [ St_Initiator_0(~id, me, peer), Init_Handshake_Initiator(~id, prologue, ~ltk, peer) ]

/* Initiator is Snding message 1 */

rule Instrumentation_Initiator_1:
 [ St_Initiator_0(~id, me, peer), In(msg) ]
 --[  ]->
 [ St_Initiator_1(~id, me, peer), Handshake_Snd_1(~id, msg) ]

