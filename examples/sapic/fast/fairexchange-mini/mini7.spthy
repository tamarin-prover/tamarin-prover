theory mini7
begin

section{* small example for progression function *}
builtins: signing, hashing, reliable-channel
options: translation-progress

event A(); ! (event B() + (!event C()) )

lemma A_possible: 
        exists-trace
        "Ex #t. A()@t"

lemma B_possible: 
        exists-trace
        "Ex #t. B()@t"

lemma C_possible: 
        exists-trace
        "Ex #t. C()@t"

lemma A_B_possible:
        exists-trace
        "Ex #t1 #t2 . A()@t1 & B()@t2"

lemma A_forced:
        "Ex #t. A()@t"


end
