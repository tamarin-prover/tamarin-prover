                              qed
                            qed
                          qed
                        next
                          case generate_ltk
                          solve( !KU( h1(<~eskI, ~lkI>) ) @ #vk.13 )
                            case c_h1
                            solve( !KU( ~eskI ) @ #vk.15 )
                              case Ephk_reveal
                              by contradiction /* from formulas */
                            qed
