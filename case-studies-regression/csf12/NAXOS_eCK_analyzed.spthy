                    next
                      case c_exp
                      solve( !KU( ~lkI ) @ #vk.14 )
                        case Ltk_reveal
                        solve( !KU( h1(<~eskR, ~lkR>) ) @ #vk.15 )
                          case c_h1
                          solve( !KU( ~eskR ) @ #vk.17 )
