              case generate_ltk
              solve( Match( ~eskR, <'Init', $I.1, $R.1, 'g'^h1(<~eskI, ~lkI>), Y>
                     ) @ #i4 )
                case Resp_1
                solve( !KU( h2(<'g'^(~x*h1(<~eskR, ~lkR>)), 
                                'g'^(~lkR*h1(<~eskI, ~lkI>)), 'g'^(h1(<~eskI, ~lkI>)*h1(<~eskR, ~lkR>)), 
