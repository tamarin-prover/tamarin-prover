                          case c_exp
                          solve( !KU( ~x ) @ #vk.15 )
                            case Ltk_reveal
                            solve( !KU( h1(<~eskI, ~lkI>) ) @ #vk.16 )
                              case c_h1
                              solve( !KU( ~eskI ) @ #vk.18 )
                                case Ephk_reveal
