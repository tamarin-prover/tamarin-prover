                      next
                        case c_exp
                        solve( !KU( ~m1 ) @ #vk.15 )
                          case Ephk_reveal
                          solve( !KU( 'g'^~m2 ) @ #vk.12 )
                            case Resp_1
                            solve( !KU( ~ltkI ) @ #vk.17 )
                              case Ltk_reveal
