                  case split_case_2
                  solve( splitEqs(1) )
                    case split_case_1
                    solve( !KU( MAC(KDF(<'g'^~m1, 'g'^(~m2*~x*inv(~ltkI)), $I, $R, 
                                         'g'^(~ltkR*~m1), 'g'^(~m2*~x)>),
