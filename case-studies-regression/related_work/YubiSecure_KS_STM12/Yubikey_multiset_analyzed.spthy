        qed
      qed
    next
      case case_2
      solve( !SharedKey( pid, k.1 ) ▶₂ #t2 )
        case BuyANewYubikey
        solve( !Y( ~pid, sid ) ▶₀ #t1 )
          case BuyANewYubikey
          solve( !SharedKey( ~pid, k ) ▶₂ #t1 )
