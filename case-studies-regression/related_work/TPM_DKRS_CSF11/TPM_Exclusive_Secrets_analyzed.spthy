  "∀ #i #j. ((UniqueInit( ) @ #j) ∧ (UniqueInit( ) @ #i)) ⇒ (#i = #j)"
  // safety formula

restriction Ineq_checks_succeed:
  "∀ t #e. (InEq( t, t ) @ #e) ⇒ (⊥)"
