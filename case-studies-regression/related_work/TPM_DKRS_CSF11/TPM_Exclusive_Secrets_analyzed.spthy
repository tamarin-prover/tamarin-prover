    case case_1
    solve( !KeyTable( d1, d2 ) ▶₁ #i )
      case PCR_CreateKey
      solve( !KU( aenc(m, pk(~ska)) ) @ #vk )
        case Alice_Init_case_1
        by contradiction /* from formulas */
      next
