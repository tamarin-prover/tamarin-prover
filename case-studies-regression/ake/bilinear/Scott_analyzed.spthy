    2. ~ey   = ~ey.36
       X     = z.41^inv(~ey.36)
       z     = z.41
    
    3. ~ey   = ~ey.372
       X     = x.739^x.740
       z     = x.739^(~ey.372*x.740)
    
    4. ~ey   = ~ey.374
       X     = x.743^inv((~ey.374*x.744))
