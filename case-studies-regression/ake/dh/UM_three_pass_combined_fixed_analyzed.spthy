            (∀ #k.
              (Corrupt( B ) @ #k)
             ⇒
              ∃ #l. (Expire( <'UM3', B, A, matchingComm> ) @ #l) ∧ #l < #k) ∧
            (∀ #k.
              (SessionStateReveal( <'UM3', B, A, matchingComm> ) @ #k)
             ⇒
              (∀ #l. (Corrupt( A ) @ #l) ⇒ ⊥) ∧ (∀ #l. (Corrupt( B ) @ #l) ⇒ ⊥))) )
