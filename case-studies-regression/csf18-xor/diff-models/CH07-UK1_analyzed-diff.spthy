builtins: multiset, xor
functions: fst/1, h/1, lh/1, pair/2, rh/1, rot/2, snd/1
equations: fst(<x.1, x.2>) = x.1, snd(<x.1, x.2>) = x.2



rule (modulo E) setup:
   [ Fr( ~k1 ), Fr( ~id1 ), Fr( ~k2 ), Fr( ~id2 ) ]
  --[ OnlyOnce( 'setup' ) ]->
   [
   TagRun( <~k1, ~id1>, 'learning' ), TagRun( <~k2, ~id2>, 'learning' ),
   TagRun( diff(<~k1, ~id1>, <~k2, ~id2>), 'guessing' )
   ]

rule (modulo E) reader1:
   [ Fr( ~r1 ) ]
  --[ Challenge( ~r1, 'Reader' ) ]->
   [ Out( ~r1 ), Initiated( ~r1 ) ]

rule (modulo E) tag1:
   [ In( r1 ), Fr( ~r2 ), TagRun( <~k, ~id>, phase ) ]
  --[ Response( ~k, 'Tag' ), Challenge( ~r2, 'Tag' ), Phase( phase ) ]->
   [
   Out( <~r2, lh((h((~k⊕~r2⊕r1))⊕rot(~id, h((~k⊕~r2⊕r1)))))> ),
   TagState( ~k, ~id, r1, ~r2, h((~k⊕~r2⊕r1)), phase )
   ]

rule (modulo E) reader2:
   [
   Initiated( ~r1 ), ReaderRun( <~k, ~id>, phase ),
   In( <r2, lh((hash⊕rot(~id, hash)))> )
   ]
  --[
  Alive( ~k, 'Tag' ), Response( ~k, 'Reader' ), Eq( h((~k⊕~r1⊕r2)), hash ),
  Phase( phase )
  ]->
   [ Out( rh((hash⊕rot(~id, hash))) ) ]

rule (modulo E) tag2:
   [
   In( rh((hash⊕rot(~id, hash))) ),
   TagState( ~k, ~id, r1, ~r2, hash, phase )
   ]
  --[ Alive( ~k, 'Reader' ), Phase( phase ) ]->
   [ ]

