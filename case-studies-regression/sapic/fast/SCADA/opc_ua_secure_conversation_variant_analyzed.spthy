builtins: multiset
functions: fst/1, h/1, mac/2, pair/2, sdec/2, senc/2, snd/1, true/0,
           verifyMac/3
equations:
    fst(<x.1, x.2>) = x.1,
    sdec(senc(x.1, x.2), x.2) = x.1,
    snd(<x.1, x.2>) = x.2,
    verifyMac(mac(m, sk), m, sk) = true

