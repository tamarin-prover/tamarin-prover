lemma agreement_B:
  all-traces
  "∀ A B t #i.
    (Commit_B( A, B, t ) @ #i) ⇒
    (((∃ #j. (Running_A( A, B, t ) @ #j) ∧ (#j < #i)) ∨
      (∃ #r. Reveal( A ) @ #r)) ∨
     (∃ #r. Reveal( B ) @ #r))"
/*
guarded formula characterizing all counter-examples:
"∃ A B t #i.
  (Commit_B( A, B, t ) @ #i)
 ∧
  (∀ #j. (Running_A( A, B, t ) @ #j) ⇒ ¬(#j < #i)) ∧
  (∀ #r. (Reveal( A ) @ #r) ⇒ ⊥) ∧
  (∀ #r. (Reveal( B ) @ #r) ⇒ ⊥)"
*/
simplify
solve( StateB( A, $B, ~kbs, kab, ~nb ) ▶₀ #i )
  case B_to_A
  solve( !Ltk( ~kbs, $B ) ▶₂ #i )
    case Init
    solve( !KU( senc(<'4', succ(~nb)>, kab) ) @ #vk )
      case A_to_B
      solve( !KU( senc(<'2', ts, A, ~kab>, ~kbs) ) @ #vk.1 )
        case S_to_B
        solve( !KU( senc(<'1', ta.1, $B, ~kab>, ~kas.1) ) @ #vk.5 )
          case A_to_S
          by contradiction /* from formulas */
        next
          case c_senc
          solve( !KU( ~kas.1 ) @ #vk.7 )
            case Reveal_Longterm_Key
            by contradiction /* from formulas */
          qed
        qed
      next
        case c_senc
        solve( !KU( ~kbs ) @ #vk.4 )
          case Reveal_Longterm_Key
          by contradiction /* from formulas */
        qed
      qed
    next
      case c_senc
      solve( !KU( senc(<'2', ts, A, kab>, ~kbs) ) @ #vk.1 )
        case S_to_B
        solve( !KU( succ(~nb) ) @ #vk.5 )
          case A_to_B
          solve( !KU( ~kab.1 ) @ #vk.10 )
            case A_to_S
            solve( !KU( ~kas.1 ) @ #vk.11 )
              case Reveal_Longterm_Key
              solve( !KU( senc(<'1', ta.1, $B, kab>, ~kas.1) ) @ #vk.10 )
                case A_to_S
                by contradiction /* from formulas */
