                next
                  case c_senc
                  solve( !KU( ~kab ) @ #vk.9 )
                    case A_to_S
                    solve( !KU( ~kas ) @ #vk.12 )
                      case Reveal_Longterm_Key
                      by contradiction /* from formulas */
                    qed
                  next
                    case S_to_B
                    solve( !KU( ~kbs.2 ) @ #vk.12 )
                      case Reveal_Longterm_Key
                      by contradiction /* from formulas */
                    qed
                  qed
                qed
              qed
            qed
          next
            case case_2
            solve( StateB( A2, $B.1, ~kbs.1, ~kab, ~nb.1 ) ▶₀ #i2 )
              case B_to_A
              solve( !Ltk( ~kbs.1, $B.1 ) ▶₂ #i2 )
                case Init
