            by contradiction /* from formulas */
          qed
        qed
      next
        case c_senc
        solve( !KU( ~kbs ) @ #vk.4 )
          case Reveal_Longterm_Key
          by contradiction /* from formulas */
        qed
      qed
    next
      case c_senc
      solve( !KU( senc(<'2', ts, A, kab>, ~kbs) ) @ #vk.1 )
        case S_to_B
        solve( !KU( succ(~nb) ) @ #vk.5 )
          case A_to_B
          solve( !KU( ~kab.1 ) @ #vk.10 )
            case A_to_S
