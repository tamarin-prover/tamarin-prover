        qed
      qed
    next
      case c_senc
      solve( !KU( senc(<'5', kab, ~nb, A>, ~kbs) ) @ #vk.1 )
        case A_to_B2
        solve( !KU( ~kab ) @ #vk.7 )
          case A_to_B2
          solve( !KU( ~kbs ) @ #vk.19 )
            case Reveal_Longterm_Key
