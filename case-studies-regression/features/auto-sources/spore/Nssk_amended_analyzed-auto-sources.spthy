        qed
      next
        case S_to_A
        solve( !KU( ~kas ) @ #vk.17 )
          case Reveal_Longterm_Key
          by contradiction /* from formulas */
        qed
      next
        case c_senc
        solve( !KU( ~kbs ) @ #vk.12 )
          case Reveal_Longterm_Key
          by contradiction /* from formulas */
