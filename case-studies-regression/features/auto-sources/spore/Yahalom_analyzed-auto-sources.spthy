                    by contradiction /* from formulas */
                  qed
                qed
              next
                case S_1_case_2
                solve( !KU( ~nb ) @ #vk.23 )
                  case A_2
                  solve( !KU( ~kxs ) @ #vk.25 )
                    case Reveal_Longterm_Key
                    by contradiction /* from formulas */
                  qed
                next
                  case B_1
                  solve( !KU( ~kxs ) @ #vk.24 )
                    case Reveal_Longterm_Key
                    by contradiction /* from formulas */
                  qed
                next
                  case S_1_case_1
                  solve( !KU( ~kxs ) @ #vk.25 )
                    case Reveal_Longterm_Key
                    by contradiction /* from formulas */
                  qed
                next
                  case S_1_case_2
                  solve( !KU( ~kxs ) @ #vk.25 )
                    case Reveal_Longterm_Key
                    by contradiction /* from formulas */
                  qed
                qed
              qed
            qed
          qed
        next
          case c_senc
          solve( !KU( ~kxs ) @ #vk.14 )
            case Reveal_Longterm_Key
            by contradiction /* from formulas */
          qed
        qed
      next
        case c_senc
        solve( !KU( ~kab ) @ #vk.10 )
          case S_1_case_1
          solve( !KU( ~nb ) @ #vk.12 )
            case A_2
            solve( !KU( ~kxs.1 ) @ #vk.14 )
              case Reveal_Longterm_Key
              by contradiction /* from formulas */
            qed
          next
            case B_1
            solve( !KU( ~kxs.1 ) @ #vk.13 )
              case Reveal_Longterm_Key
              by contradiction /* from formulas */
            qed
          next
            case S_1_case_1
            solve( !KU( ~kxs.1 ) @ #vk.14 )
              case Reveal_Longterm_Key
