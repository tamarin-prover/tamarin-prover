          qed
        next
          case case_2
          solve( !LongtermKey( ~kas.1, $A.1 ) ▶₀ #i2 )
            case Init
            solve( StateA( $A.1, $B.1, ~m, ~na.1, ~kas.1 ) ▶₂ #i2 )
              case A_1
              by contradiction /* cyclic */
            qed
          qed
        qed
      next
        case c_senc
        solve( !KU( ~kas ) @ #vk.15 )
          case Reveal_Longterm_Key
          by contradiction /* from formulas */
        qed
      qed
    next
      case c_senc
      solve( !KU( ~kas ) @ #vk.4 )
        case Reveal_Longterm_Key
        by contradiction /* from formulas */
      qed
    qed
  qed
qed

