lemma injectiveagreement_S:
  all-traces
  "(∀ A B t #i.
     (Commit_S_A( A, B, t ) @ #i) ⇒
     (((∃ #j.
         ((Running_A( A, B, t ) @ #j) ∧ (#j < #i)) ∧
         (¬(∃ A2 B2 #i2. (Commit_S_A( A2, B2, t ) @ #i2) ∧ (¬(#i2 = #i))))) ∨
       (∃ #r. Reveal( A ) @ #r)) ∨
      (∃ #r. Reveal( B ) @ #r))) ∧
   (∀ A B t #i.
     (Commit_S_B( A, B, t ) @ #i) ⇒
     (((∃ #j.
         ((Running_B( A, B, t ) @ #j) ∧ (#j < #i)) ∧
         (¬(∃ A2 B2 #i2. (Commit_S_B( A2, B2, t ) @ #i2) ∧ (¬(#i2 = #i))))) ∨
       (∃ #r. Reveal( A ) @ #r)) ∨
      (∃ #r. Reveal( B ) @ #r)))"
/*
guarded formula characterizing all counter-examples:
"((∃ A B t #i.
    (Commit_S_A( A, B, t ) @ #i)
   ∧
    (∀ #j.
      (Running_A( A, B, t ) @ #j)
     ⇒
      ((¬(#j < #i)) ∨
       (∃ A2 B2 #i2. (Commit_S_A( A2, B2, t ) @ #i2) ∧ ¬(#i2 = #i)))) ∧
    (∀ #r. (Reveal( A ) @ #r) ⇒ ⊥) ∧
    (∀ #r. (Reveal( B ) @ #r) ⇒ ⊥)) ∨
  (∃ A B t #i.
    (Commit_S_B( A, B, t ) @ #i)
   ∧
    (∀ #j.
      (Running_B( A, B, t ) @ #j)
     ⇒
      ((¬(#j < #i)) ∨
       (∃ A2 B2 #i2. (Commit_S_B( A2, B2, t ) @ #i2) ∧ ¬(#i2 = #i)))) ∧
    (∀ #r. (Reveal( A ) @ #r) ⇒ ⊥) ∧
    (∀ #r. (Reveal( B ) @ #r) ⇒ ⊥)))"
*/
induction
  case non_empty_trace
  simplify
  solve( (∃ A B t #i.
           (Commit_S_A( A, B, t ) @ #i)
          ∧
           (∀ #j.
             (Running_A( A, B, t ) @ #j)
            ⇒
             ((¬(#j < #i)) ∨
              (∃ A2 B2 #i2. (Commit_S_A( A2, B2, t ) @ #i2) ∧ ¬(#i2 = #i)))) ∧
           (∀ #r. (Reveal( A ) @ #r) ⇒ ⊥) ∧
           (∀ #r. (Reveal( B ) @ #r) ⇒ ⊥))  ∥
         (∃ A B t #i.
           (Commit_S_B( A, B, t ) @ #i)
          ∧
           (∀ #j.
             (Running_B( A, B, t ) @ #j)
            ⇒
             ((¬(#j < #i)) ∨
              (∃ A2 B2 #i2. (Commit_S_B( A2, B2, t ) @ #i2) ∧ ¬(#i2 = #i)))) ∧
           (∀ #r. (Reveal( A ) @ #r) ⇒ ⊥) ∧
           (∀ #r. (Reveal( B ) @ #r) ⇒ ⊥)) )
    case case_2
    solve( (last(#i))  ∥
           (∃ #j.
             (Running_A( A, B, <m, na> ) @ #j)
