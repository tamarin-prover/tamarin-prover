                    solve( !KU( pk(~ltkY) ) @ #vk.28 )
                      case AS_send_key
                      solve( !KU( aenc(<'4', ~n2>, pk(~ltkS)) ) @ #vk.31 )
                        case C_to_S
                        solve( !KU( pk(~ltkS) ) @ #vk.35 )
                          case AS_send_key
                          SOLVED // trace found
                        qed
                      qed
                    qed
                  qed
                qed
              qed
            qed
          qed
        qed
      qed
    qed
  qed
qed

lemma injectiveagreement_A:
  all-traces
  "∀ A B t #i.
    (Commit_A( A, B, t ) @ #i) ⇒
    ((((∃ #j.
         ((Running_B( A, B, t ) @ #j) ∧ (#j < #i)) ∧
         (¬(∃ A2 B2 #i2. (Commit_A( A2, B2, t ) @ #i2) ∧ (¬(#i2 = #i))))) ∨
