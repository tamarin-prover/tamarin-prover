      case AS_send_key
      solve( !KU( aenc(<'5', $S, inc(~n2)>, pk(~ltkC)) ) @ #vk.8 )
        case c_aenc
        solve( !KU( pk(~ltkC) ) @ #vk.20 )
          case AS_send_key
          solve( !KU( inc(~n2) ) @ #vk.24 )
            case S_to_C
            solve( !KU( sign(<'Key', 'AS', $S.1, ~n3, pk(ltkC.1)>, ~ltkAS)
                   ) @ #vk.37 )
              case AS_send_key
